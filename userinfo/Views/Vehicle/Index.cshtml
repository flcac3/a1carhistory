        <h2>Add Vehicle</h2>
        <form>
            <div class="form-group">
                <label for="vin">VIN</label>
                <input type="text" id="vin" placeholder="Enter VIN Number" required>
            </div>

            <div class="form-group">
                <label for="make">Make</label>
                <input type="text" id="make" placeholder="Enter Make">
            </div>

            <div class="form-group">
                <label for="model">Model</label>
                <input type="text" id="model" placeholder="Enter Model">
            </div>

            <div class="form-group">
                <label for="trim">Trim</label>
                <input type="text" id="trim" placeholder="Enter Trim">
            </div>

            <div class="form-group">
                <label for="modelYear">Model Year</label>
                <input type="text" id="modelYear" placeholder="Enter Year">
            </div>

            <div class="form-group">
                <label for="engineCylinders">Engine Cylinders</label>
                <input type="text" id="engineCylinders" placeholder="Enter number of Cylinders">
            </div>

            <div class="form-group">
                <label for="displacementL">Displacement L</label>
                <input type="text" id="displacementL" placeholder="Enter displacement in liters">
            </div>

            <div class="form-group">
                <label for="engineHP">Engine HP</label>
                <input type="text" id="engineHP" placeholder="Enter HP">
            </div>

            <div class="form-group">
                <label for="driveType">Drive Type</label>
                <input type="text" id="driveType" placeholder="Enter driveType">
            </div>

            <div class="form-group">
                <label for="mileage">Mileage</label>
                <input type="number" id="mileage" required placeholder="Enter mileage">
            </div>

            <div class="form-group">
                <label for="purchaseDate">Purchase Date</label>
                <input type="date" id="purchaseDate" required placeholder="Enter purchase date">
            </div>

            <div class="form-group">
                <label for="nickname">Nickname</label>
                <input type="text" id="nickname" required placeholder="Enter nickname">
            </div>

            <div class="form-group">
                <label for="carPhoto">Car Photo</label>
                <input type="file" id="carPhoto" placeholder="Upload photo of car">
            </div>

            <button type="submit">Add Vehicle</button>
        </form>
